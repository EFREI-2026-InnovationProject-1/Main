{
	"git.autofetch": true,
	"git.autofetchPeriod": 60,
	"git.enableSmartCommit": true,
	"git.postCommitCommand": "sync",
	"git.pruneOnFetch": true,
	"git.terminalGitEditor": true,
	"git.useCommitInputAsStashMessage": true,
	"git.useEditorAsCommitInput": false,
	"githubPullRequests.notifications": "pullRequests",
	"githubPullRequests.quickDiff": true,
	"githubPullRequests.defaultCreateOption": "createDraft",
	"githubIssues.issueBranchTitle": "issue/${issueNumber}",
	"githubIssues.issueCompletionFormatScm": "Closes ${issueNumberLabel}",
	"githubIssues.workingIssueFormatScm": "Contributes ${issueNumberLabel}",
	"terminal.integrated.suggest.enabled": true,
	"chat.agent.enabled": true,
	"github.copilot.chat.agent.thinkingTool": true,
	"github.copilot.chat.codesearch.enabled": true,
	"github.copilot.chat.completionContext.typescript.mode": "sidecar",
	"github.copilot.chat.editor.temporalContext.enabled": true,
	"github.copilot.chat.edits.temporalContext.enabled": true,
	"github.copilot.chat.generateTests.codeLens": true,
	"github.copilot.chat.languageContext.fix.typescript.enabled": true,
	"github.copilot.chat.languageContext.inline.typescript.enabled": true,
	"github.copilot.chat.languageContext.typescript.enabled": true,
	"githubPullRequests.experimental.chat": true,
	"githubPullRequests.experimental.notificationsView": true,
	"githubPullRequests.experimental.useQuickChat": true,
	"nuxtr.defaultPackageManager": "pnpm",
	"npm.packageManager": "pnpm"
}
